            New "Frame" {
                BackgroundColor3 = themeProvider:GetColor(Enum.StudioStyleGuideColor.DiffFilePathBorder, Enum.StudioStyleGuideModifier.Default),
                BorderSizePixel = 0,
                Size = UDim2.new(1, 0, 0, 1),
                Position = UDim2.fromScale(0, 1),
            }
        },
    }
